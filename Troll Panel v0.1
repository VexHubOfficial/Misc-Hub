                    local bv=Instance.new("BodyVelocity",root)
                    bv.MaxForce=Vector3.new(1e5,1e5,1e5)
                    bv.Velocity=(targetRoot.Position-root.Position).Unit*50+Vector3.new(0,50,0)
                    Debris:AddItem(bv,0.2)
                end
            end
        end
    end
end)

